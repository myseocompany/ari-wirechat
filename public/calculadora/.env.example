# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key

# WhatsApp Integration (Optional)
WHATSAPP_TOKEN=your_whatsapp_business_token
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id
WHATSAPP_PROVIDER=whatsapp_business

# Twilio Integration (Alternative to WhatsApp Business API)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# SellerChat Integration (Optional)
SELLERCHAT_API_KEY=your_sellerchat_api_key
SELLERCHAT_WEBHOOK_URL=https://api.sellerchat.com/webhooks/leads

# Analytics (Optional)
VITE_GA_TRACKING_ID=UA-XXXXX-X
VITE_FB_PIXEL_ID=XXXXX
VITE_GTM_ID=GTM-XXXXX
VITE_HOTJAR_ID=XXXXX

# Feature Flags (Optional)
VITE_ENABLE_CHAT=false
VITE_ENABLE_PDF_DOWNLOAD=true
VITE_ENABLE_WHATSAPP_AUTO=false

# Environment
ENVIRONMENT=development

# Branding (Optional)
VITE_COMPANY_NAME=Tu Empresa
VITE_COMPANY_LOGO_URL=https://example.com/logo.png
VITE_SUPPORT_EMAIL=soporte@empresa.com
VITE_SUPPORT_PHONE=+57 300 123 4567

# Rate Limiting (Optional)
VITE_MAX_SUBMISSIONS_PER_IP=5
VITE_RATE_LIMIT_WINDOW=3600
